{
    "name": "atagursel/polling-system",
    "version": "1.0.0",
    "description": "Modern, güvenli ve özellik dolu PHP tabanlı anket sistemi",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Ata Gürsel"
        }
    ],
    "keywords": [
        "anket",
        "poll",
        "survey",
        "php",
        "mysql",
        "chart",
        "javascript"
    ],
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Config\\": "config/"
        },
        "files": [
            "config/config.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "serve": "php -S localhost:8000 -t public",
        "test": "echo 'Tests not implemented yet'",
        "lint": "php -l src/**/*.php"
    },
    "support": {
        "issues": "https://github.com/ATAGRSL/polling-system/issues",
        "source": "https://github.com/ATAGRSL/polling-system"
    }
}
